<Window
    x:Class="SMM.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SMM"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Space Team Murder Mystery"
    Width="854"
    Height="480"
    MinWidth="854"
    MinHeight="480"
    Icon="pack://application:,,,/Assets/Images/nebula.ico"
    Style="{StaticResource DarkTheme}">
    <Grid Margin="18,10">
        <ContentControl
            x:Name="MainContent"
            Grid.Row="0"
            Grid.Column="0" />
    </Grid>
</Window>
